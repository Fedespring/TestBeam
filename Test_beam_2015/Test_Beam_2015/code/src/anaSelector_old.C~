	TRKAResY=      new TH2F("TRKAResY","",100,-1.,1.,10,-12.,28.);  
	TRKAResQ=      new TH2F("TRKAResQ","",100,-1.,1.,200,0,5000.);
	TRKAYQ=      new TH2F("TRKAYQ","",10,-12.,28.,200,0,5000.);
	TRKAResChi2=  new TH2F("TRKAResChi2","",100,-1.,1.,3,0.,3.);
